<%= back_link_path course_students_path(course_id) %>
<%= render 'title' %>

<%= simple_form_for [course, @student] do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :surname %>
  <%= f.input :dni, label: 'DNI' %>
  <%= f.input :email %>
  <%= f.input :file_number %>
  <%= f.button :submit %>
<% end %>